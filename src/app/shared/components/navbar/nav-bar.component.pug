mdl-layout-header-row
    mdl-layout-title Home
    mdl-layout-spacer
    mdl-textfield(type='text', icon='search')
    button(mdl-button, '#navbarmenubtn'='mdlButton', (click)='navbarmenu.toggle($event, navbarmenubtn)', mdl-button-type='icon', mdl-ripple)
        mdl-icon more_vert
    mdl-menu('#navbarmenu'='mdlMenu', mdl-menu-position='bottom-right') 
        mdl-menu-item('*ngFor'='let entry of moreBtnEntries', mdl-ripple) {{ entry.title }}
